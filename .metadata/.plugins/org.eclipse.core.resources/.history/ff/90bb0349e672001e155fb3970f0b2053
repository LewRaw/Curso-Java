package entities;

public class Student {
	public String name;
	public double t1;
	public double t2;
	public double t3;
	
	public double grades() {
		return t1 + t2 + t3;
	}
	public String checkPassed() {
		return (grades() >= 60)? "PASS":"FAILED" + System.lineSeparator() + "MISSING " +(60 - grades())+" POINTS";
	}
}
